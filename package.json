    "@emotion/styled": "^11.3.0",
    "@govtechsg/decentralized-renderer-react-components": "^3.5.3",
    "@govtechsg/oa-encryption": "^1.3.3",
    "@govtechsg/oa-verify": "^7.4.3",
    "@govtechsg/open-attestation": "^5.2.2",
    "next": "^12.0.3",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-feather": "^2.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@commitlint/prompt": "^12.1.1",
    "@emotion/eslint-plugin": "^11.2.0",
    "@ls-age/commitlint-circle": "^1.0.0",
    "@storybook/addon-actions": "^6.2.0",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "commitizen": "^4.2.4",
    "eslint": "^7.25.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^4.1.2",
    "git-cz": "^4.7.6",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.3",
    "serve": "^11.3.2",
    "tailwindcss": "^2.1.2",
    "testcafe": "^1.14.0",
    "typescript": "^4.2.4",
    "zlib": "^1.0.5"
  },
  "scripts": {
    "build": "next build",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "build:next": "next build",
    "commit": "git-cz",
    "commit:retry": "npm run commit -- --retry",
    "dev": "next dev",
    "eject": "react-scripts eject",
    "integration": "testcafe chrome integration/**/*.spec.ts --ts-config-path ./tsconfig.testcafe.json",
    "integration:headless": "testcafe chrome:headless integration/**/*.spec.ts --ts-config-path ./tsconfig.testcafe.json --app \"serve ./build --listen 3000 --single\" --app-init-delay 5000",
    "lint": "next lint",
    "lint:fix": "npm run lint -- --fix",
    "monitoring": "testcafe chrome:headless monitoring/**/*.spec.ts --ts-config-path ./tsconfig.testcafe.json",
    "start": "next start",
    "start:css": "postcss src/tailwind.css -o src/index.css --watch",
    "start:next": "next start",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "npm run build:css && build-storybook -s public",
    "test": "react-scripts test --passWithNoTests"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
