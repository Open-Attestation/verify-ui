  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "integration": "testcafe 'chrome --use-fake-ui-for-media-stream --use-fake-device-for-media-stream' integration/*.spec.ts --hostname localhost --compilerOptions noEmit=true",
    "integration:headless": "testcafe 'chrome --headless=new --use-fake-ui-for-media-stream --use-fake-device-for-media-stream' integration/*.spec.ts --hostname localhost --compilerOptions noEmit=true --app 'npm start' --app-init-delay 5000",
    "integration:multi-window-test:headless": "testcafe 'chrome --headless=new --use-fake-ui-for-media-stream --use-fake-device-for-media-stream' integration/multi-window-test/*.spec.ts --hostname localhost --compilerOptions noEmit=true --app 'npm start' --app-init-delay 5000  --disable-native-automation",
    "monitoring": "testcafe chrome:headless monitoring/**/*.spec.ts --skip-js-errors --compiler-options typescript.options.esModuleInterop=true"
  },
